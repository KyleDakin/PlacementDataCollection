{
  "name": "Test Tensorflow Extension",
  "version": "1.0",
  "description": "Testing development of an extension for Object Detection using Tensorflow",

  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "*://*/*",
    "<all_urls>",
    "tabs"
  ],

  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },

  "browser_action": {
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },

  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },

  "content_scripts": [
    {
      "js": [ "content.js" ],
      "matches": [ "http://127.0.0.1/*" ]
    }
  ],
  "manifest_version": 2
}